<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="4"
	parallel="tests" verbose="4">
	
	<listeners>
	<listener class-name = "com.qa.opencart.Listeners.ExtentReportListener" />
	<listener class-name= "com.qa.opencart.Listeners.TestAllureListener" />
	<listener class-name= "com.qa.opencart.Listeners.AnnotationTransformer" />

	</listeners>





	<test name="Login Page Test_chrome_ver">
		<parameter name="browser" value="chrome"></parameter>
		<parameter name="browserversion" value="90.0"></parameter>
		<parameter name="testcasename" value="Login Page Test ver"></parameter>
		<classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>







</suite>	
